package com.superkeyword;

 public class SuperClassMethodCallingMain extends SuperClassMethodCalling {
	 void m2()
	 {
		 System.out.println("this is child class m2 method");
	 }
	 void show()
	 {
		 this.m2();
		 super.m1();
	 }
	 public static void main(String[] args) {
		 SuperClassMethodCallingMain spc=new SuperClassMethodCalling();
		 spc.show();
	}
 
}
